# BigQuery Configuration
DBT_BIGQUERY_PROJECT=your-gcp-project-id
DBT_BIGQUERY_DATASET=your_dataset_name
DBT_BIGQUERY_LOCATION=US
DBT_BIGQUERY_KEYFILE_PATH=/app/credentials/gcp-key.json

# Dagster Configuration
DAGSTER_HOME=/opt/dagster/dagster_home

# Optional: Airbyte Configuration (if using Airbyte integration)
AIRBYTE_HOST=localhost
AIRBYTE_PORT=8000
AIRBYTE_USERNAME=airbyte
AIRBYTE_PASSWORD=password

# Optional: Database Configuration (if using external databases)
DATABASE_URL=postgresql://user:password@localhost:5432/dagster

# Development Configuration
DEBUG=true
LOG_LEVEL=info
